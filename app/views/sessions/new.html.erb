<body>
<div class = "container">
<h1>Login</h1>
<%= form_for @user, url: sessions_path do |f| %>
	<%= f.email_field "user_email", user[:email], placeholder: "Email" %><br>
	<%= f.password_field "user_password", user placeholder: "PWD" %><br>
	<%= f.submit "Login" %>
<% end %>
</div>
</body>